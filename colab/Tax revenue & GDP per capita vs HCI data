        "\n",
        "df_clean['Total tax revenue per capita'] = df_clean['GDP (current US$)'] * df_clean['Tax revenue % of GDP'] / 100 / df_clean['Population']\n",
        "df_clean['GDP per capita'] = df_clean['GDP (current US$)'] / df_clean['Population']\n",
        "df_clean\n"
